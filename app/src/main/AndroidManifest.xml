<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rahulkadavil.buggyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_buggy_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_buggy_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.Buggyapp">
        <activity android:name=".description"/>
        <activity android:name=".Vulnerability" />
        <activity android:name=".Flagchecker" />
        <activity
            android:exported="false"
            android:name=".login"
            android:parentActivityName=".MainActivity">
            <meta-data

                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />

        </activity>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>